<resources>
    <!-- Base application theme. -->
    <style name="Theme.HealthAppCollegeProject" parent="Theme.MaterialComponents.DayNight.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/blue</item>
        <item name="colorPrimaryVariant">@color/blue_light</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/violet</item>
        <item name="colorSecondaryVariant">@color/violet_light</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" >?attr/colorPrimary</item>
        <!-- Customize your theme here. -->
        <item name="backgroundColor">@color/light</item>
        
        <item name="android:fontFamily">@font/poppins</item>
    </style>
</resources>